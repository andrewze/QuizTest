# platform :ios, '11.4'

target 'Quiz' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Quiz
  pod 'Realm'
  pod 'RealmSwift'
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'ObjectMapper'
  pod 'AlamofireObjectMapper'
  pod "ObjectMapper+Realm"

  inhibit_all_warnings!
end
